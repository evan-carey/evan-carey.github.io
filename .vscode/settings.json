{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.exclude": {
        "**/*.map": true,
        "**/node_modules": true,
        "index.html": true,
        "app.css": true,
        "app.js": true,
        "polyfills.js": true,
        "vendor.js": true
    },
    "search.exclude": {
        "**/*.map": true,
        "**/node_modules": true,
        "index.html": true,
        "app.css": true,
        "app.js": true,
        "polyfills.js": true,
        "vendor.js": true
    },
    "tslint.run": "onSave",
    "autoimport.filesToScan": "src/**/*.ts",
    "autoimport.spaceBetweenBraces": true,
    "genGetSet.spacedImportLine": true,
    "genGetSet.scanOnSave": true,
    "genGetSet.filter": true,
    "genGetSet.ignoredDictionaryList": [
        "vscode",
        "tsconfig"
    ],
    "genGetSet.pathStringDelimiter": "'",
    "genGetSet.ignoredNodeLibraries": [
        "node",
        "chrome"
    ],
    "genGetSet.importTypings": false,
    "genGetSet.importNode": true
}